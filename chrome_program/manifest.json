{
  "name": "translation reader",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "chrome translator",
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "css/bootstrap.min.css",
        "css/mystyle.css"
      ],
      "js": [
        "script/jquery.min.js",
        "script/bootstrap.min.js",
        "script/md5.min.js",
        "script/youdao.server.model.js",
        "script/translator.js"
      ]
    }
  ]
}